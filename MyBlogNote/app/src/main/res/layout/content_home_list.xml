<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_home_list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.maxfilippi.myblognote.activity.HomeListActivity"
    tools:showIn="@layout/app_bar_home_list">

    
    <android.support.v7.widget.RecyclerView
        android:id="@+id/home_list_recycler_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:paddingTop="10dp"
        app:layoutManager="LinearLayoutManager"
        tools:listitem="@layout/row_item_note"/>


    
</RelativeLayout>
